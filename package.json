{
  "name": "nextcloud-tasks-webextension",
  "version": "0.0.1",
  "description": "Nextcloud Tasks App Browser Extension",
  "private": true,
  "scripts": {
    "start": "webpack --progress --colors --watch",
    "build": "webpack --progress --colors --env.production",
    "sonar": "sonar-scanner -Dsonar.host.url=${SONAR_HOST_URL} -Dsonar.login=${SONAR_TOKEN} -Dsonar.organization=riscue -Dsonar.javascript.lcov.reportPaths=coverage/lcov.info -Dsonar.coverage.exclusions=**/*.spec.ts",
    "bom": "npm run bom:generate && npm run bom:upload",
    "bom:generate": "cyclonedx-bom",
    "bom:upload": "dependency-track -u bom.xml --dependencyTrackBaseUrl ${DEPENDENCY_TRACK_URL} --apiKey ${SECRET_DEPENDENCY_TRACK_API_KEY} --projectName ${npm_package_name} --projectVersion ${npm_package_version}",
    "test": "ts-node -r tsconfig-paths/register node_modules/jasmine/bin/jasmine.js --config=jasmine.json",
    "coverage": "nyc -r text -r lcov npm run test"
  },
  "dependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.21.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.0.0",
    "ical.js": "^1.4.0",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "^1.3.3",
    "moment": "^2.26.0",
    "node-sass": "^5.0.0",
    "raw-loader": "^4.0.1",
    "sass-loader": "^11.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "ua-parser-js": "^0.7.21",
    "url-loader": "^4.1.0",
    "vue": "^2.6.11",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-property-decorator": "^9.1.2",
    "vue-style-loader": "^4.1.2",
    "vuelidate": "^0.7.5",
    "webextension-polyfill-ts": "~0.22.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  },
  "devDependencies": {
    "@cyclonedx/bom": "^2.0.2",
    "@dependency-track/bom": "^1.0.2",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/jasmine": "^3.6.2",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node": "^15.14.0",
    "@types/ua-parser-js": "^0.7.33",
    "@types/vuelidate": "^0.7.13",
    "@types/webpack": "^5.0.0",
    "jasmine": "^3.6.3",
    "localstorage-polyfill": "^1.0.1",
    "nyc": "^15.1.0",
    "sonarqube-scanner": "^2.8.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-loader": "^3.5.4",
    "tslint-sonarts": "^1.9.0"
  },
  "keywords": [
    "tasks",
    "nextcloud",
    "schedule",
    "extension"
  ],
  "author": "Ä°brahim Akyel",
  "license": "GPL"
}
